<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<import resource="baseBeans.xml"/>
	
	<bean id="defaultTclExecutor" factory-bean="tclExecutorFactory"	factory-method="getTclExecutor">
		<constructor-arg value="defaultTclExecutor"/> 
	</bean>
	
	<bean id="executorA" factory-bean="tclExecutorFactory"	factory-method="getTclExecutor">
		<constructor-arg value="TclInterpreter-A"/> 
	</bean>
	<bean id="executorB" factory-bean="tclExecutorFactory"	factory-method="getTclExecutor">
		<constructor-arg value="TclInterpreter-B"/> 
	</bean>

</beans>
